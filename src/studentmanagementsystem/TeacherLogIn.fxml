<AnchorPane id="AnchorPane" prefHeight="478.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="studentmanagementsystem.TeacherLogInController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="162.0" layoutX="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@220px-RUET_logo.svg.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="254.0" layoutY="39.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images%20(1).jpg" />
         </image>
      </ImageView>
      <Label alignment="TOP_LEFT" layoutX="109.0" layoutY="245.0" prefHeight="44.0" prefWidth="106.0" text="User Id :">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="TeacherId" layoutX="215.0" layoutY="235.0" prefHeight="44.0" prefWidth="274.0" />
      <Label alignment="TOP_LEFT" contentDisplay="CENTER" layoutX="104.0" layoutY="322.0" prefHeight="30.0" prefWidth="116.0" text="Password :">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <PasswordField fx:id="TeacherPassword" layoutX="215.0" layoutY="313.0" prefHeight="44.0" prefWidth="274.0" />
      <Button layoutX="220.0" layoutY="388.0" mnemonicParsing="false" onAction="#loginButton" prefHeight="25.0" prefWidth="116.0" text="Log In">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Button layoutX="375.0" layoutY="388.0" mnemonicParsing="false" onAction="#CancelButton" text="Cancel">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Label fx:id="wrong" layoutX="18.0" layoutY="389.0" prefHeight="44.0" prefWidth="185.0">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
